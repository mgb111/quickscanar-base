{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/python/mindar.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/python/(.*)",
      "dest": "/api/python/mindar.py"
    }
  ],
  "functions": {
    "api/python/mindar.py": {
      "runtime": "python3.9"
    }
  }
} 